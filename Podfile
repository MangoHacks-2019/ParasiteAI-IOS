  pod 'AWSS3'
  pod 'SwiftyJSON'
  pod 'Apollo'
  pod 'SwiftLint', '0.27'
  pod 'Auth0'
  pod 'EightBase'

  target 'ApolloAITests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ApolloAIUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
